<Window x:Class="W_P_Fornicator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:W_P_Fornicator"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnPancho" Content="Pancho" HorizontalAlignment="Center" Margin="0,217,0,0" VerticalAlignment="Top" Height="77" Width="196" Foreground="Yellow" BorderBrush="{x:Null}" Click="btnPancho_Click" FontFamily="Consolas" FontWeight="Normal" FontStyle="Italic" Visibility="Visible" Cursor="Pen">
            <Button.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFE2C5B5"/>
                    <GradientStop Color="#FF21FBD3" Offset="1"/>
                </LinearGradientBrush>
            </Button.Background>
        </Button>

    </Grid>
</Window>
